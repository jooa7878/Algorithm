#include <iostream>
#include <string>

using namespace std;


int main() {
	int N;
	string S;
	int R;
	

	cin >> N;
	for (int i = 0; i < N; i++) {
		cin >> R >> S;
		string result;
		for (int j = 0; j < S.length(); j++) {
			for (int k = 0; k < R; k++) {
				result += S[j];
			}
		}
		cout << result << endl;
	}
}	
